<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weizu.mymusicdemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/kg_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlayMusicDemo">
        <activity android:name=".MainActivity"/>
        <activity android:name=".activitys.WelcomeActivity"
            android:theme="@style/WelcomeThemeBefore"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".services.MyLaunchMusicService"/>
    </application>

</manifest>